#, qt-format
msgid "Failed to save file “%1”: %2"
msgstr "A(z) „%1” fájl mentése sikertelen: %2"

#: ../src/filetree.cc:927
#, qt-format
msgid "“%1” seems to be a binary file."
msgstr "„%1” bináris fájlnak tűnik."

#: ../src/main.cc:79
msgid "Save _all"
msgstr "Összes _mentése"

#: ../src/main.cc:136
msgid "Find files matching PATTERN"
msgstr "A MINTÁRA illeszkedő fájlok keresése"

#: ../src/main.cc:136
msgid "PATTERN"
msgstr "MINTA"

#: ../src/main.cc:138
msgid "Do not recurse into subdirectories"
msgstr "Alkönyvtárakba ne menjen le"

#: ../src/main.cc:140
msgid "Also find hidden files"
msgstr "Rejtett fájlokat is keres"

#: ../src/main.cc:142
msgid "Find text matching REGEX"
msgstr "A REGEXRE illeszkedő fájlok keresése"

#: ../src/main.cc:142
msgid "REGEX"
msgstr "REGEX"

#: ../src/main.cc:144
msgid "Find only the first match in a line"
msgstr "Csak az első találat keresése egy sorban"

#: ../src/main.cc:146
msgid "Do case insensitive matching"
msgstr "A kis- és nagybetűk nem különböznek"

#: ../src/main.cc:148
msgid "Replace matches with STRING"
msgstr "Találatok helyettesítése a KARAKTERLÁNCCAL"

#: ../src/main.cc:148
msgid "STRING"
msgstr "KARAKTERLÁNC"

#: ../src/main.cc:150
msgid "Print match location to standard output"
msgstr "Találat helyének kiírása a szabvány kimenetre"

#: ../src/main.cc:152
msgid "Do not automatically start search"
msgstr "Ne kezdjen keresni automatikusan"

#: ../src/main.cc:154
msgid "[FOLDER]"
msgstr "[MAPPA]"

#: ../src/mainwindow.cc:513
msgid ""
"Some files haven’t been saved yet.\n"
"Quit anyway?"
msgstr ""
"Néhány fájl még nincs elmentve.\n"
"Mindenképp kilép?"

#: ../src/mainwindow.cc:527
msgid ""
"Some files haven’t been saved yet.\n"
"Continue anyway?"
msgstr ""
"Néhány fájl még nincs elmentve.\n"
"Mindenképp folytatja?"

