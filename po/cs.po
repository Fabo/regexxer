#: ../ui/regexxer.desktop.in.h:1 ../src/mainwindow.cc:967
msgid "Search and replace using regular expressions"
msgstr "Hledat a nahradit pomocí regulárních výrazů"

#: ../ui/regexxer.desktop.in.h:2
msgid "regexxer Search Tool"
msgstr "Nástroj pro hledání regexxer"

#: ../ui/regexxer.schemas.in.h:1
msgid "Current-match color"
msgstr "Barva aktuálního výskytu"

#: ../ui/regexxer.schemas.in.h:2
msgid "Fallback encoding"
msgstr "Náhradní kódování"

#: ../ui/regexxer.schemas.in.h:3
msgid "List of last pattern used for the file's regex entry."
msgstr "Seznam naposledy použitých vzorů v poli vzorek pro soubor."

#: ../ui/regexxer.schemas.in.h:4
msgid "List of last pattern used for the regex entry."
msgstr "Seznam naposledy použitých vzorů v poli hledat."

#: ../ui/regexxer.schemas.in.h:5
msgid "List of last pattern used for the substitution entry."
msgstr "Seznam naposledy použitých vzorů v poli nahradit."

#: ../ui/regexxer.schemas.in.h:6
msgid "Match color"
msgstr "Barva výskytu"

#: ../ui/regexxer.schemas.in.h:7
msgid ""
"Name of the character encoding to use if a file is not readable in either "
"UTF-8 or the codeset specified by the current locale. Try “iconv --list” for "
"a complete list of possible values."
msgstr ""
"Název kódování znaků, které se má použít v případě, že soubor není čitelný "
"ani v UTF-8 ani v kódování určeném současným národním nastavením. Pro úplný "
"seznam možných hodnot zkuste „iconv --list“."

#: ../ui/regexxer.schemas.in.h:8
msgid "Regex Patterns"
msgstr "Vzor regex:"

#: ../ui/regexxer.schemas.in.h:9
msgid "Text view font"
msgstr "Písmo pro zobrazení textu"

#: ../ui/regexxer.schemas.in.h:10
msgid ""
"The background color used to highlight matches of the search expression."
msgstr "Barva pozadí používaná pro zvýraznění výskytů hledaného výrazu."

#: ../ui/regexxer.schemas.in.h:11
msgid ""
"The background color used to highlight the currently selected match of the "
"search expression."
msgstr ""
"Barva pozadí používaná pro zvýraznění aktuálně vybraného výskytu hledaného "
"výrazu."

#: ../ui/regexxer.schemas.in.h:12
msgid "The font used in the file editor."
msgstr "Písmo používané v editoru souborů."

#: ../src/filebuffer.cc:198
msgid "Can’t read file:"
msgstr "Nelze číst soubor:"

#: ../src/filetree.cc:61
msgid "File"
msgstr "Soubor"

#: ../src/filetree.cc:80
msgid "#"
msgstr "#"

#: ../src/filetree.cc:511
#, qt-format
msgid "Failed to save file “%1”: %2"
msgstr "Selhalo uložení souboru „%1“: %2"

#: ../src/filetree.cc:927
#, qt-format
msgid "“%1” seems to be a binary file."
msgstr "„%1“ je zřejmě binární soubor."

#: ../src/main.cc:79
msgid "Save _all"
msgstr "Uložit _vše"

#: ../src/main.cc:136
msgid "Find files matching PATTERN"
msgstr "Hledat soubory odpovídající VZORU"

#: ../src/main.cc:136
msgid "PATTERN"
msgstr "VZOR"

#: ../src/main.cc:138
msgid "Do not recurse into subdirectories"
msgstr "Nezanořovat se do podadresářů"

#: ../src/main.cc:140
msgid "Also find hidden files"
msgstr "Hledat i skryté soubory"

#: ../src/main.cc:142
msgid "Find text matching REGEX"
msgstr "Hledat text odpovídající REGEX"

#: ../src/main.cc:142
msgid "REGEX"
msgstr "REGEX"

#: ../src/main.cc:144
msgid "Find only the first match in a line"
msgstr "Hledat pouze první výskyt na řádku"

#: ../src/main.cc:146
msgid "Do case insensitive matching"
msgstr "Nerozlišovat velikost písmen"

#: ../src/main.cc:148
msgid "Replace matches with STRING"
msgstr "Nahradit výskyty ŘETĚZCEM"

#: ../src/main.cc:148
msgid "STRING"
msgstr "ŘETĚZEC"

#: ../src/main.cc:150
msgid "Print match location to standard output"
msgstr "Vypsat umístění výskytů na standardní výstup"

#: ../src/main.cc:152
msgid "Do not automatically start search"
msgstr "Nezačínat hledání automaticky"

#: ../src/main.cc:154
msgid "[FOLDER]"
msgstr "[SLOŽKA]"

#: ../src/mainwindow.cc:513
msgid ""
"Some files haven’t been saved yet.\n"
"Quit anyway?"
msgstr ""
"Některé soubory dosud nebyly uloženy.\n"
"Přesto skončit?"

#: ../src/mainwindow.cc:527
msgid ""
"Some files haven’t been saved yet.\n"
"Continue anyway?"
msgstr ""
"Některé soubory dosud nebyly uloženy.\n"
"Přesto pokračovat?"

#: ../src/mainwindow.cc:559
msgid "The file search pattern is invalid."
msgstr "Vzor pro hledání souborů je neplatný."

#: ../src/mainwindow.cc:565
msgid "The following errors occurred during search:"
msgstr "Při hledání došlo k následujícím chybám:"

